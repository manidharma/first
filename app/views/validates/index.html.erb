<p id="notice"><%= notice %></p>

<h1>Validates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Password</th>
      <th>Email</th>
      <th>City</th>
      <th>State</th>
      <th>Pincode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @validates.each do |validate| %>
      <tr>
        <td><%= validate.name %></td>
        <td><%= validate.password %></td>
        <td><%= validate.email %></td>
        <td><%= validate.city %></td>
        <td><%= validate.state %></td>
        <td><%= validate.pincode %></td>
        <td><%= link_to 'Show', validate %></td>
        <td><%= link_to 'Edit', edit_validate_path(validate) %></td>
        <td><%= link_to 'Destroy', validate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Validate', new_validate_path %>
